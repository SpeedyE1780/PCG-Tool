<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="True">
    <editor:ObjectField label="Start Node" name="SequenceNode" type="UnityEngine.ScriptableObject, UnityEngine.CoreModule" tooltip="First node in sequence" />
    <engine:TextField label="File Name" placeholder-text="File Name" name="FileName" tooltip="Name of generated sequence SO" />
    <engine:TextField label="Folder Path" placeholder-text="Folder Path" name="FolderPath" tooltip="Path of generated SO" />
    <engine:Toggle label="Cyclic Generation" name="CyclicToggle" tooltip="Enable Cyclic Sequence" />
    <engine:UnsignedIntegerField label="SequenceLength" value="0" name="SequenceLength" tooltip="Length of sequence" />
    <engine:UnsignedIntegerField label="Seed" value="0" name="Seed" tooltip="PCG API Seed" />
    <engine:Button text="Generate" name="Generate" tooltip="Generate Sequence" />
</engine:UXML>
